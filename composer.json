{
  "name"              : "valkyrja/valkyrja",
  "type"              : "project",
  "description"       : "The Valkyrja Framework.",
  "homepage"          : "https://www.valkyrja.io",
  "keywords"          : [
    "framework",
    "valkyrja"
  ],
  "license"           : "MIT",
  "authors"           : [
    {
      "name"  : "Melech Mizrachi",
      "email" : "melechmizrachi@gmail.com"
    }
  ],
  "require"           : {
    "php"                     : ">=8.4",
    "psr/container"           : "^2.0.2",
    "psr/event-dispatcher"    : "^1.0.0",
    "psr/http-message"        : "^2.0",
    "psr/http-server-handler" : "^1.0.2",
    "psr/log"                 : "^3.0.2"
  },
  "require-dev"       : {
    "filp/whoops"                : "^2.18.4",
    "firebase/php-jwt"           : "^7.0.2",
    "guzzlehttp/guzzle"          : "^7.10.0",
    "league/flysystem"           : "^3.30.2",
    "league/flysystem-aws-s3-v3" : "^3.30.1",
    "mailgun/mailgun-php"        : "^4.3.6",
    "monolog/monolog"            : "^3.10.0",
    "phpmailer/phpmailer"        : "^7.0.2",
    "predis/predis"              : "^3.3.0",
    "pusher/pusher-php-server"   : "^7.2.7",
    "twig/twig"                  : "^3.22.2",
    "vonage/client-core"         : "^4.12.0"
  },
  "conflict"          : {
    "filp/whoops"                : "<2.18.4",
    "firebase/php-jwt"           : "<7.0.2",
    "guzzlehttp/guzzle"          : "<7.10.0",
    "league/flysystem"           : "<3.30.2",
    "league/flysystem-aws-s3-v3" : "<3.30.1",
    "mailgun/mailgun-php"        : "<4.3.6",
    "monolog/monolog"            : "<3.10.0",
    "phpmailer/phpmailer"        : "<7.0.2",
    "predis/predis"              : "<3.3.0",
    "pusher/pusher-php-server"   : "<7.2.7",
    "twig/twig"                  : "<3.22.2",
    "vonage/client-core"         : "<4.12.0"
  },
  "autoload"          : {
    "psr-4" : {
      "Valkyrja\\" : "src/Valkyrja"
    }
  },
  "autoload-dev"      : {
    "psr-4" : {
      "Valkyrja\\Tests\\" : "tests"
    },
    "files" : [
      "functions/helpers.php"
    ]
  },
  "bin"               : [
    "bin/valkyrja"
  ],
  "suggest"           : {
    "filp/whoops"                : "Required to use the default Throwable handler (^2.18.4)",
    "firebase/php-jwt"           : "Required to use the default Jwt adapter (^7.0.2)",
    "guzzlehttp/guzzle"          : "Required to use the default Client adapter (^7.10.0).",
    "league/flysystem"           : "Required to use the default Filesystem adapter (^3.30.2).",
    "league/flysystem-aws-s3-v3" : "Required to use the s3 Filesystem adapter (^3.30.1)",
    "mailgun/mailgun-php"        : "Required to use the Mailgun Mail adapter (^4.3.6).",
    "monolog/monolog"            : "Required to use the default Log adapter (^3.9.0).",
    "phpmailer/phpmailer"        : "Required to use the default Mail adapter (^7.0.2).",
    "predis/predis"              : "Required to use the Redis Cache store (^3.3.0).",
    "pusher/pusher-php-server"   : "Required to use the Pusher Broadcast adapter (^7.2.7).",
    "twig/twig"                  : "Required to use the Twig View engine (^3.22.2).",
    "vonage/client-core"         : "Required to use the default SMS adapter (^4.12.0).",
    "ext-curl"                   : "Required to use the default Client adapter (*)",
    "ext-pdo"                    : "Required to use the default ORM adapter (*)",
    "ext-redis"                  : "Required to use the default Cache store (^4.0|^5.0).",
    "ext-sodium"                 : "Required to use the default Crypt adapters and/or JWT EdDSA Algorithm (*)"
  },
  "scripts"           : {
    "churn"                                           : "cd .github/ci/churn && composer check",
    "churn-outdated-check"                            : "cd .github/ci/churn && composer install && composer outdated --direct --minor-only",
    "churn-outdated-check-major-release"              : "cd .github/ci/churn && composer install && composer outdated --direct",
    "churn-composer-update"                           : "cd .github/ci/churn && composer update",
    "phparkitect"                                     : "cd .github/ci/phparkitect && composer check",
    "phparkitect-outdated-check"                      : "cd .github/ci/phparkitect && composer install && composer outdated --direct --minor-only",
    "phparkitect-outdated-check-major-release"        : "cd .github/ci/phparkitect && composer install && composer outdated --direct",
    "phparkitect-composer-update"                     : "cd .github/ci/phparkitect && composer update",
    "phpcodesniffer"                                  : "cd .github/ci/phpcodesniffer && composer check",
    "phpcodesniffer-outdated-check"                   : "cd .github/ci/phpcodesniffer && composer install && composer outdated --direct --minor-only",
    "phpcodesniffer-outdated-check-major-release"     : "cd .github/ci/phpcodesniffer && composer install && composer outdated --direct",
    "phpcodesniffer-composer-update"                  : "cd .github/ci/phpcodesniffer && composer update",
    "phpcsfixer"                                      : "cd .github/ci/phpcsfixer && composer fix",
    "phpcsfixer-check"                                : "cd .github/ci/phpcsfixer && composer check",
    "phpcsfixer-outdated-check"                       : "cd .github/ci/phpcsfixer && composer install && composer outdated --direct --minor-only",
    "phpcsfixer-outdated-check-major-release"         : "cd .github/ci/phpcsfixer && composer install && composer outdated --direct",
    "phpcsfixer-composer-update"                      : "cd .github/ci/phpcsfixer && composer update",
    "phpmetrics"                                      : "cd .github/ci/phpmetrics && composer check",
    "phpmetrics-outdated-check"                       : "cd .github/ci/phpmetrics && composer install && composer outdated --direct --minor-only",
    "phpmetrics-outdated-check-major-release"         : "cd .github/ci/phpmetrics && composer install && composer outdated --direct",
    "phpmetrics-composer-update"                      : "cd .github/ci/phpmetrics && composer update",
    "phpstan"                                         : "cd .github/ci/phpstan && composer check",
    "phpstan-outdated-check"                          : "cd .github/ci/phpstan && composer install && composer outdated --direct --minor-only",
    "phpstan-outdated-check-major-release"            : "cd .github/ci/phpstan && composer install && composer outdated --direct",
    "phpstan-composer-update"                         : "cd .github/ci/phpstan && composer update",
    "phpunit"                                         : "cd .github/ci/phpunit && composer check",
    "phpunit-coverage"                                : "cd .github/ci/phpunit && composer coverage",
    "phpunit-no-cov"                                  : "cd .github/ci/phpunit && composer no-coverage",
    "phpunit-outdated-check"                          : "cd .github/ci/phpunit && composer install && composer outdated --direct --minor-only",
    "phpunit-outdated-check-major-release"            : "cd .github/ci/phpunit && composer install && composer outdated --direct",
    "phpunit-composer-update"                         : "cd .github/ci/phpunit && composer update",
    "psalm"                                           : "cd .github/ci/psalm && composer check",
    "psalm-no-cache"                                  : "cd .github/ci/psalm && composer no-cache",
    "psalm-shepherd"                                  : "cd .github/ci/psalm && composer shepherd",
    "psalm-shepherd-with-stats"                       : "cd .github/ci/psalm && composer shepherd-with-stats",
    "psalm-stats"                                     : "cd .github/ci/psalm && composer stats",
    "psalm-outdated-check"                            : "cd .github/ci/psalm && composer install && composer outdated --direct --minor-only",
    "psalm-outdated-check-major-release"              : "cd .github/ci/psalm && composer install && composer outdated --direct",
    "psalm-composer-update"                           : "cd .github/ci/psalm && composer update",
    "rector"                                          : "cd .github/ci/rector && composer fix",
    "rector-check"                                    : "cd .github/ci/rector && composer check",
    "rector-outdated-check"                           : "cd .github/ci/rector && composer install && composer outdated --direct --minor-only",
    "rector-outdated-check-major-release"             : "cd .github/ci/rector && composer install && composer outdated --direct",
    "rector-composer-update"                          : "cd .github/ci/rector && composer update",
    "root-outdated-check"                             : "composer install && composer outdated --direct --minor-only",
    "root-outdated-check-major-release"               : "composer install && composer outdated --direct",
    "root-and-suggested-outdated-check"               : "cd .github/ci/root-and-suggested && composer install && composer outdated --direct --minor-only",
    "root-and-suggested-outdated-check-major-release" : "cd .github/ci/root-and-suggested && composer install && composer outdated --direct",
    "root-and-suggested-composer-update"              : "cd .github/ci/root-and-suggested && composer update"
  },
  "minimum-stability" : "dev",
  "prefer-stable"     : true,
  "extra"             : {
    "branch-alias" : {
      "dev-master" : "25.x-dev"
    }
  },
  "config"            : {
    "allow-plugins" : {
      "php-http/discovery" : false
    }
  }
}
