includes:
    - phpstan-baseline.neon
parameters:
    level: 9
    bootstrapFiles:
        - ../../../vendor/autoload.php
    paths:
        - ../../../src
#        - tests
    treatPhpDocTypesAsCertain: false
    ignoreErrors:
        - '#^Unsafe usage of new static\(\).$#'
        -
            identifier: missingType.generics
        -
            identifier: trait.unused
