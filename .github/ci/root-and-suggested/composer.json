{
  "require"           : {
    "php"                     : ">=8.4",
    "psr/container"           : "^2.0.2",
    "psr/event-dispatcher"    : "^1.0.0",
    "psr/http-message"        : "^2.0",
    "psr/http-server-handler" : "^1.0.2",
    "psr/log"                 : "^3.0.2"
  },
  "require-dev"       : {
    "filp/whoops"                : "^2.18.4",
    "firebase/php-jwt"           : "^7.0.2",
    "guzzlehttp/guzzle"          : "^7.10.0",
    "league/flysystem"           : "^3.30.2",
    "league/flysystem-aws-s3-v3" : "^3.30.1",
    "mailgun/mailgun-php"        : "^4.3.5",
    "monolog/monolog"            : "^3.10.0",
    "phpmailer/phpmailer"        : "^6.12.0",
    "predis/predis"              : "^2.4.1",
    "pusher/pusher-php-server"   : "^7.2.7",
    "twig/twig"                  : "^3.22.2",
    "vonage/client-core"         : "^4.11.2"
  },
  "minimum-stability" : "dev",
  "prefer-stable"     : true,
  "config"            : {
    "allow-plugins" : {
      "php-http/discovery" : true
    }
  }
}
