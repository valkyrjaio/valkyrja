{
  "require"           : {
    "php" : ">=8.4"
  },
  "require-dev"       : {
    "rector/rector" : "^2.3.4"
  },
  "scripts"           : {
    "fix"   : "vendor/bin/rector",
    "check" : "vendor/bin/rector --dry-run"
  },
  "autoload"          : {
    "psr-4" : {
      "Valkyrja\\Rector\\" : "Rector"
    }
  },
  "minimum-stability" : "dev",
  "prefer-stable"     : true
}
