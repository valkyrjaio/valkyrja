{
  "require"           : {
    "php" : ">=8.4"
  },
  "require-dev"       : {
    "firebase/php-jwt"            : "^6.11.1",
    "guzzlehttp/guzzle"           : "^7.10.0",
    "league/flysystem"            : "^3.30.2",
    "league/flysystem-aws-s3-v3"  : "^3.30.1",
    "mailgun/mailgun-php"         : "^4.3.5",
    "phpmailer/phpmailer"         : "^6.12.0",
    "predis/predis"               : "^2.4.1",
    "pusher/pusher-php-server"    : "^7.2.7",
    "twig/twig"                   : "^3.22.1",
    "vonage/client-core"          : "^4.11.2"
  },
  "minimum-stability" : "dev",
  "prefer-stable"     : true,
  "config"            : {
    "allow-plugins" : {
      "php-http/discovery" : true
    }
  }
}
